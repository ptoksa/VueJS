{"ast":null,"code":"import { mapActions, mapGetters } from 'vuex';\nexport default {\n  name: 'TaskList',\n  data() {\n    return {\n      newTaskTitle: ''\n    };\n  },\n  computed: {\n    ...mapGetters(['tasks'])\n  },\n  methods: {\n    ...mapActions(['addTask', 'deleteTask', 'toggleTaskCompletion']),\n    submitTask() {\n      if (this.newTaskTitle) {\n        this.addTask({\n          name: this.newTaskTitle,\n          completed: false\n        });\n        this.newTaskTitle = '';\n      }\n    },\n    toggleCompletion(taskId) {\n      this.toggleTaskCompletion(taskId);\n    },\n    deleteTask(taskId) {\n      this.$store.dispatch('deleteTask', taskId);\n    }\n  },\n  created() {\n    this.$store.dispatch('fetchTasks');\n  }\n};","map":{"version":3,"names":["mapActions","mapGetters","name","data","newTaskTitle","computed","methods","submitTask","addTask","completed","toggleCompletion","taskId","toggleTaskCompletion","deleteTask","$store","dispatch","created"],"sources":["/home/petri/Ohjelmointi/VueJS/task-manager/src/components/TaskList.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>Task List</h1>\n    <ul>\n      <li v-for=\"task in tasks\" :key=\"task.id\">\n        {{ task.name }}\n        <button @click=\"toggleCompletion(task.id)\">Toggle Complete</button>\n        <button @click=\"deleteTask(task.id)\">Delete</button>\n      </li>\n    </ul>\n    <input v-model=\"newTaskTitle\" placeholder=\"New task title\" />\n    <button @click=\"submitTask\">Add Task</button>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex';\n\nexport default {\n  name: 'TaskList',\n  data() {\n    return {\n      newTaskTitle: ''\n    };\n  },\n  computed: {\n    ...mapGetters(['tasks'])\n  },\n  methods: {\n    ...mapActions(['addTask', 'deleteTask', 'toggleTaskCompletion']),\n    submitTask() {\n      if (this.newTaskTitle) {\n        this.addTask({ name: this.newTaskTitle, completed: false });\n        this.newTaskTitle = '';\n      }\n    },\n    toggleCompletion(taskId) {\n      this.toggleTaskCompletion(taskId);\n    },\n    deleteTask(taskId) {\n      this.$store.dispatch('deleteTask', taskId);\n    }\n  },\n  created() {\n    this.$store.dispatch('fetchTasks');\n  }\n};\n</script>\n"],"mappings":"AAgBA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGJ,UAAU,CAAC,CAAC,OAAO,CAAC;EACzB,CAAC;EACDK,OAAO,EAAE;IACP,GAAGN,UAAU,CAAC,CAAC,SAAS,EAAE,YAAY,EAAE,sBAAsB,CAAC,CAAC;IAChEO,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACH,YAAY,EAAE;QACrB,IAAI,CAACI,OAAO,CAAC;UAAEN,IAAI,EAAE,IAAI,CAACE,YAAY;UAAEK,SAAS,EAAE;QAAM,CAAC,CAAC;QAC3D,IAAI,CAACL,YAAW,GAAI,EAAE;MACxB;IACF,CAAC;IACDM,gBAAgBA,CAACC,MAAM,EAAE;MACvB,IAAI,CAACC,oBAAoB,CAACD,MAAM,CAAC;IACnC,CAAC;IACDE,UAAUA,CAACF,MAAM,EAAE;MACjB,IAAI,CAACG,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAEJ,MAAM,CAAC;IAC5C;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,MAAM,CAACC,QAAQ,CAAC,YAAY,CAAC;EACpC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}